{{ ansible_warning ['#'] }}

description "wistla {{ node_type }}"

start on runlevel [2345]
stop on runlevel [!2345]

task

script

	. /config/environment

	for index in $(seq 0 $((WISTLA_{{ node_type | upper }}_COUNT - 1))); do
		start wistla-{{ node_type }}-instance WISTLA_{{ node_type | upper }}_INDEX=$index
	done

end script
