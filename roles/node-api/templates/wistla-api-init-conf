description "wistla api"

start on runlevel [2345]
stop on runlevel [!2345]

env NODE_ENV=vagrant

script

	cd /home/ubuntu/wistla-api

	setuid ubuntu \
	mkdir --parents logs

	setuid ubuntu \
	{{ 'node-debug' if debug == 'yes' else 'nodejs' }} server.js \
		> logs/output.txt 2> logs/output.txt

end script
