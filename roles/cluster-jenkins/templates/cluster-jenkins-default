NAME="jenkins"

JAVA="/usr/bin/java"
JAVA_ARGS=""
JAVA_ARGS="$JAVA_ARGS -Djava.awt.headless=true"
JAVA_ARGS="$JAVA_ARGS -Dfile.encoding=UTF8"

PIDFILE="/var/run/jenkins/jenkins.pid"
JENKINS_LOG="/var/log/jenkins/$NAME.log"

JENKINS_USER="ubuntu"
JENKINS_GROUP="ubuntu"
JENKINS_HOME="/home/ubuntu"

JENKINS_WAR="/usr/share/jenkins/jenkins.war"

RUN_STANDALONE="true"
MAXOPENFILES="8192"

HTTP_PORT="8080"
AJP_PORT="-1"
PREFIX="/jenkins"

JENKINS_ARGS=""
JENKINS_ARGS="$JENKINS_ARGS --webroot=/var/cache/jenkins/war"
JENKINS_ARGS="$JENKINS_ARGS --httpPort=$HTTP_PORT"
JENKINS_ARGS="$JENKINS_ARGS --ajp13Port=$AJP_PORT"
