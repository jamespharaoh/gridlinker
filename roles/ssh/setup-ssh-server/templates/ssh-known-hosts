{%- for host_name in groups.all -%}

	{%- set host_data = hostvars [host_name] -%}

	{%- if host_data.ssh_host_key_rsa is defined -%}

		{%- print [
			[
				host_name,
				host_data.private_address,
			] | join (','),
			host_data.ssh_host_key_rsa,
		] | join (' ') + '\n' -%}

	{%- endif -%}

	{%- if host_data.ssh_host_key_ecdsa is defined -%}

		{%- print [
			[
				host_name,
				host_data.private_address,
			] | join (','),
			host_data.ssh_host_key_ecdsa,
		] | join (' ') + '\n' -%}

	{%- endif -%}

{%- endfor -%}
