# run rc as well

source "$HOME/.bashrc"

# set language

export LANGUAGE="C.UTF-8"
export LANG="C.UTF-8"
export LC_ALL="C.UTF-8"
export LC_NAME="C.UTF-8"

# set path

export PATH="$HOME/bin:$PATH"

# postgres connection

export PGHOST="{{ devbox_private }}"
export PGPORT="5001"
export PGUSER="postgres"
export PGPASSWORD="topsecret"

# ex: noet ts=4 filetype=sh
