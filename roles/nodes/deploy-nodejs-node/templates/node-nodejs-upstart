{{ ansible_warning ['#'] }}

description "wistla {{ node_template }}"

start on runlevel [2345]
stop on runlevel [!2345]

task

script

	. /config/environment

	for index in $(seq 0 $((WISTLA_{{ node_template | upper }}_COUNT - 1))); do
		start {{ item.0.name }}-instance WISTLA_{{ node_template | upper }}_INDEX=$index
	done

end script
