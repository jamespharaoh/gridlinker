{{ ansible_warning ['#'] }}

# change to wistla-devops directory

cd "$HOME/wistla-devops"

# run rc as well

source "$HOME/.bashrc"

# set language

export LANGUAGE=C
export LANG=C
export LC_ALL=C
export LC_NAME=C

# set paths

NODE="/opt/node-v{{ nodejs_version }}-linux-x64"

export MANPATH="$NODE/share/man:$(/usr/bin/manpath --quiet)"

# ex: noet ts=4 filetype=sh
