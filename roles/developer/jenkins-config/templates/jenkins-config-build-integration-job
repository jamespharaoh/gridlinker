  <builders>
    <hudson.tasks.Shell>
      <command><![CDATA[{{ [
        '#!/bin/bash',
        '',
        'set -euf -o pipefail',
        '',
        'sudo rm -rf "shared"',
        '',
        'sudo git clean -xdf',
        '',
        'git checkout "' + jenkins_build.name + '-build";',
        '',
        [
          'git merge',
          '--strategy recursive',
          '--strategy-option theirs',
          'origin/' + jenkins_build.name + '-merged',
        ] | join (' '),
        '',
        'exec jenkins-scripts/3-build-integration ' + jenkins_build.name,
      ] | join ('\n') }}]]></command>
